# Algorand Network Configuration
# Use one of: mainnet, testnet, betanet, or localnet
ALGORAND_NETWORK=testnet

# Algod API Configuration
# For TestNet (public node - no token needed):
ALGOD_ADDRESS=https://testnet-api.algonode.cloud
ALGOD_TOKEN=

# For LocalNet (when running algokit localnet start):
# ALGOD_ADDRESS=http://localhost:4001
# ALGOD_TOKEN=aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa

# Creator Account Configuration
# WARNING: Never commit real private keys to version control!
# For testing, you can generate a test account with: algokit generate account
CREATOR_ADDRESS=YOUR_ALGORAND_ADDRESS_HERE
CREATOR_PRIVATE_KEY=YOUR_BASE64_PRIVATE_KEY_HERE

# Optional: Resolver Account (if different from creator)
RESOLVER_ADDRESS=
RESOLVER_PRIVATE_KEY=

# Optional: Dispenser Account (for funding test accounts)
DISPENSER_ADDRESS=
DISPENSER_PRIVATE_KEY=

# Application Configuration
APP_ID=
OUTCOME_A_ASA_ID=
OUTCOME_B_ASA_ID=
